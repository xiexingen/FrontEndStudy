{
  "compilerOptions": {
    "strict": true,
    "target": "esnext",
    "module": "esnext",
    "moduleResolution": "node",
    "importHelpers": true,
    "jsx": "react-jsx",
    "skipLibCheck": true,
    "esModuleInterop": true,
    "baseUrl": "./",
    "paths": {
      "@@/*": [".dumi/tmp/*"]
    },
    "allowSyntheticDefaultImports": true,
    "noImplicitAny": false
  },
  "include": [".dumi/**/*", ".dumirc.ts","typings.d"],
}
